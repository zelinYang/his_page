<?xml version="1.0" encoding="UTF-8"?>

<beans  xmlns="http://www.springframework.org/schema/beans"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
         default-lazy-init="true"
         >
    <bean id="dicSysType" class="cn.com.base.dictionary.support.SimpleDictionaryItem">
        <property name="displayName" value="应用系统名称"/><!--对应sys_function表中sys_type-->
<!-- 以key=value方式表示一个字典元素，多个字典元素之间以换行分隔，key和value中不能使用“=” -->
        <property name="dictionaryDatas">
            <value>
              01=HIS系统
              02=业务系统
            </value>
        </property>
    </bean>

	<!-- 用于main.vm https验证 -->
	<bean id="dicHttpsUseSite" class="cn.com.base.dictionary.support.SimpleDictionaryItem">
        <property name="displayName" value="https"/>
        <property name="dictionaryDatas">
              <value>
              	https.use.site=#{https.use.site}
              </value>
       </property>
    </bean>
</beans>
